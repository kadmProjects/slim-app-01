{
  "name": "slim-app-01",
  "version": "1.0.0",
  "description": "This is first CRUD application using PHP slim framework.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "concat:css": "concat -o public/build/css/app.min.css node_modules/bootstrap/dist/css/bootstrap.css",
    "concat:js": "concat -o public/build/js/app.min.js node_modules/jquery/dist/jquery.js",
    "minify:js": "minify > public/build/js/app.min.js node_modules/jquery/dist/jquery.js",
    "minify:css": "minify > public/build/css/app.min.css node_modules/bootstrap/dist/css/bootstrap.css",
    "uglify:css": "uglifycss > public/build/css/app.min.css node_modules/bootstrap/dist/css/bootstrap.css",
    "uglify:js": "uglifyjs -o public/build/js/app.min.js node_modules/jquery/dist/jquery.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadmProjects/slim-app-01.git"
  },
  "keywords": [],
  "author": "kadmprojects",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kadmProjects/slim-app-01/issues"
  },
  "homepage": "https://github.com/kadmProjects/slim-app-01#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "concat": "^1.0.3",
    "jquery": "^3.5.0",
    "minify": "^4.1.1",
    "popper.js": "^1.15.0",
    "uglify-js": "^3.5.14",
    "uglifycss": "0.0.29"
  }
}
